{
    "name": "monocart-reporter-test",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "main": "",
    "scripts": {
        "link": "sf link ../monocart-reporter -f",
        "radom": "node ./scripts/random.js",
        "open": "node ./scripts/open.js",
        "test": "node ./scripts/test.js && npm run merge",
        "merge": "node ./scripts/merge.js",
        "serve": "npx monocart serve-report ./scripts/demo/",
        "ten": "npx playwright test -c tests/ten-minutes",
        "coverage": "npx playwright test -c tests/coverage",
        "video": "npx playwright test -c tests/multiple-video",
        "global-state": "npx playwright test -c tests/global-state",
        "email": "npx playwright test -c integrations/email",
        "build": "npm run build-istanbul && npm run build-v8",
        "build-istanbul": "npx webpack --config packages/coverage/webpack.config-istanbul.js",
        "build-v8": "npx webpack --config packages/coverage/webpack.config-v8.js"
    },
