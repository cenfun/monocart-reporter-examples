{
    "name": "monocart-reporter-examples",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "main": "",
    "scripts": {
        "link": "sf link ../monocart-reporter -f",
        "random": "node ./scripts/random.js",
        "open": "node ./scripts/open.js",
        "test": "node ./scripts/test.js",
        "ten": "npx playwright test -c tests/ten-minutes",
        "video": "npx playwright test -c tests/multiple-video",
        "state": "npx playwright test -c tests/global-state",
        "serve": "npx monocart serve-report ./scripts/serve/",
        "audit": "npx playwright test -c tests/audit",
        "merge": "node ./scripts/merge.js",
        "merge-cli": "npx monocart merge ./docs/shard*/index.json -o docs/merged-cli -n Merged",
        "merge-ts": "cross-env NODE_OPTIONS=\"--loader ts-node/esm --no-warnings\" monocart merge ./docs/shard*/index.json",
        "shard1": "npx playwright test -c tests/shard1",
        "shard2": "npx playwright test -c tests/shard2",
        "shard3": "npx playwright test -c tests/shard3",
        "shard": "node --trace-warnings ./scripts/shard.js && npm run merge"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cenfun/monocart-reporter-examples.git"
    },
    "devDependencies": {
        "@playwright/test": "^1.47.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "eight-colors": "^1.3.0",
        "eslint": "^9.10.0",
        "eslint-config-plus": "^2.0.2",
        "eslint-plugin-html": "^8.1.1",
        "lighthouse": "^12.2.1",
        "monocart-reporter": "^2.8.1",
        "open": "^10.1.0",
        "playwright-har": "^0.2.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.2"
    }
}